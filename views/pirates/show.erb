<h1>Pirate</h1>
    <div>
        <h3>Name: <%= @pirate.name %></h3>
        <h4>Weight: <%= @pirate.weight %></h4>
        <h4>Height<%= @pirate.height %></h4>
    </div>

<h2>Ships</h2>
<% @ships.each do |ship| %>
    <p>Ship Name: <%= ship.name%></p>
    <p>Ship Type: <%= ship.type%></p>
    <p>Ship Booty: <%= ship.booty%></p>
    <br>
<% end %>
